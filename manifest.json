{
  "manifest_version": 3,
  "name": "Delta Flight Search Automation",
  "version": "1.0",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://travel.delta.com/*"],
      "js": ["content.js"]
    }
  ]
}
